{{ 'partial-shopify-product-reviews.css' | asset_url | stylesheet_tag }}

{% liquid
  assign current_variant = product.selected_or_first_available_variant

  if section.settings.show_featured_media_first and product.selected_variant == blank
    assign featured_media = product.featured_media
  else
    assign featured_media = current_variant.featured_media | default: product.featured_media
  endif

  assign first_3d_model = product.media | where: 'media_type', 'model' | first
  assign gallery_style = section.settings.gallery_style
  assign gallery_style_mobile = section.settings.gallery_style_mobile
  assign gallery_size = section.settings.gallery_size
  assign gallery_overflow = section.settings.gallery_overflow
  assign gallery_overflow_mobile = section.settings.gallery_overflow
  assign image_aspect_ratio = section.settings.image_aspect_ratio
  assign header_block = section.blocks | where: 'type', 'product_header'

  if gallery_style == 'thumbnails'
    assign gallery_overflow = 'never'
  endif

  if gallery_style_mobile == 'carousel'
    assign gallery_overflow_mobile = 'never'
  endif

  # If there's only a single image in the gallery, display as list
  if product.media.size == 1
    assign gallery_style = 'list'
    assign gallery_style_mobile = 'list'
  endif

  case gallery_size
    when 'small'
      assign desktop_image_container_percentage = 46
    when 'medium'
      assign desktop_image_container_percentage = 56
    when 'large'
      assign desktop_image_container_percentage = 64
    when 'xlarge'
      assign desktop_image_container_percentage = 72
  endcase

  if gallery_style == 'grid'
    assign desktop_image_percentage_width = desktop_image_container_percentage | divided_by: 2
  else
    assign desktop_image_percentage_width = desktop_image_container_percentage
  endif

  if section.settings.enable_multiple_variant_media
    assign media_delimiters_positions = product.variants | map: 'featured_media' | uniq | map: 'position'

    if current_variant.featured_media
      assign initial_variant_media_index = current_variant.featured_media.position | minus: 1
      assign initial_variant_media_position = current_variant.featured_media.position

      for delimiter_position in media_delimiters_positions
        if delimiter_position == initial_variant_media_position
          if media_delimiters_positions[forloop.index] == blank
            assign next_variant_media_index = product.media.last.position
            assign next_variant_media_position = product.media.last.position | plus: 1
          else
            assign next_variant_media_index = media_delimiters_positions[forloop.index] | minus: 1
            assign next_variant_media_position = media_delimiters_positions[forloop.index]
          endif
          break
        endif
      endfor

      assign media_group_size = next_variant_media_position | minus: initial_variant_media_position
    endif
  endif

  assign media_size = media_group_size | default: product.media.size
  assign media_show_limit = media_group_size | default: product.media.size
  assign over_media_show_limit = false
  assign over_media_show_limit_mobile = false

  # Unless the desktop gallery overflow is 'never', or the desktop display is 'thumbnails', initialize
  # media overflow variables
  assign media_limit = 0
  unless gallery_overflow == 'never'
    assign media_per_row = 1

    if gallery_style == 'grid'
      assign media_per_row = 2
    endif

    assign gallery_overflow_number = gallery_overflow | times: 1.0
    assign media_limit = gallery_overflow_number | times: media_per_row

    # add 1 because n+1th row should be partially visible
    assign media_show_limit = gallery_overflow_number | plus: 1 | times: media_per_row

    assign transition_multiplier = media_size | times: 1.0 | divided_by: media_per_row | ceil | minus: gallery_overflow_number | times: media_per_row | at_most: 4
    assign overflow_gallery_aspect_ratio = 'auto'

    if media_limit < media_size
      assign over_media_show_limit = true
    endif

    if image_aspect_ratio == 'natural'
      capture overflow_gallery_aspect_ratio
        render 'product-media-overflow-aspect', product: product, media_show_limit: media_show_limit, media_per_row: media_per_row
      endcapture
    else
      case image_aspect_ratio
        when 'portrait'
          assign image_fill_aspect_ratio = 0.66666
        when 'landscape'
          assign image_fill_aspect_ratio = 1.5
        when 'square'
          assign image_fill_aspect_ratio = 1
      endcase

      # Multiply by aspect ratio to maintain a constant height for the tease row
      #   Using a constant percentage would vary with image height
      assign tease_row_ratio = 0.35 | times: image_fill_aspect_ratio | times: media_per_row

      assign height = gallery_overflow_number | plus: tease_row_ratio
      assign width = image_fill_aspect_ratio | times: media_per_row
      assign overflow_gallery_aspect_ratio = width | divided_by: height
    endif
  endunless

  # Unless the mobile gallery overflow is 'never', or the mobile display is 'carousel', initialize
  # media overflow variables
  unless gallery_overflow_mobile == 'never'
    assign media_per_row_mobile = 1

    if gallery_style_mobile == 'grid'
      assign media_per_row_mobile = 2
    endif

    assign gallery_overflow_number_mobile = gallery_overflow_mobile | times: 1.0
    assign media_limit_mobile = gallery_overflow_number_mobile | times: media_per_row_mobile

    # add 1 because n+1th row should be partially visible
    assign media_show_limit_mobile = gallery_overflow_number_mobile | plus: 1 | times: media_per_row_mobile

    assign transition_multiplier_mobile = media_size | times: 1.0 | divided_by: media_per_row_mobile | ceil | minus: gallery_overflow_number_mobile | times: media_per_row_mobile | at_most: 4
    assign overflow_gallery_aspect_ratio_mobile = 'auto'

    if media_limit_mobile < media_size
      assign over_media_show_limit_mobile = true
    endif

    if image_aspect_ratio == 'natural'
      capture overflow_gallery_aspect_ratio_mobile
        render 'product-media-overflow-aspect', product: product, media_show_limit: media_show_limit_mobile, media_per_row: media_per_row_mobile
      endcapture
    else
      case image_aspect_ratio
        when 'portrait'
          assign image_fill_aspect_ratio_mobile = 0.66666
        when 'landscape'
          assign image_fill_aspect_ratio_mobile = 1.5
        when 'square'
          assign image_fill_aspect_ratio_mobile = 1
      endcase

      # Multiply by aspect ratio to maintain a constant height for the tease row
      #   Using a constant percentage would vary with image height
      assign tease_row_ratio_mobile = 0.35 | times: image_fill_aspect_ratio_mobile | times: media_per_row_mobile

      assign height_mobile = gallery_overflow_number_mobile | plus: tease_row_ratio_mobile
      assign width_mobile = image_fill_aspect_ratio_mobile | times: media_per_row_mobile
      assign overflow_gallery_aspect_ratio_mobile = width_mobile | divided_by: height_mobile
    endif
  endunless

  # Offset the show limit for multiple variant mode
  assign media_show_limit = media_show_limit | plus: initial_variant_media_index

  # Account for gutter between columns and page outer space
  capture desktop_image_sizes_attribute
    echo desktop_image_percentage_width | append: 'vw'
  endcapture

  capture product_url
    echo shop.url | append: product.url
  endcapture

  assign featured_media_size = featured_media.width | append: 'x'

  if product.featured_media
    capture product_featured_image_url
      echo product.featured_media | image_url: featured_media_size | prepend: 'https:' | json
    endcapture
  endif

  assign price_content_type = 'price'
  assign custom_price_content = blank

  if current_variant.available
    if current_variant.price == 0 and current_variant.compare_at_price == null
      if settings.zero_dollar_product_price_display == 'hide'
        assign price_content_type = 'hide'
      elsif settings.zero_dollar_product_price_display == 'replace'
        assign price_content_type = 'custom'
        assign custom_price_content = 'products.inventory.zero_price_custom_label' | t
      endif
    endif
  else
    if settings.sold_out_product_price_display == 'hide'
      assign price_content_type = 'hide'
    elsif settings.sold_out_product_price_display == 'replace'
      assign price_content_type = 'custom'
      assign custom_price_content = 'products.inventory.sold_out_price_custom_label' | t
    endif
  endif

  assign show_mobile_carousel_images_full_width = section.settings.show_mobile_carousel_images_full_width

  # Hiding mobile carousel dots is only allowed if not full-width as we consider
  # this choice only valid if there is a "peek" to indicate swipability
  assign hide_mobile_carousel_dots = false

  unless show_mobile_carousel_images_full_width
    assign hide_mobile_carousel_dots = section.settings.hide_mobile_carousel_dots
  endunless
-%}
{% style %}
  {% if gallery_style == 'carousel' %}
    .product .product__media-item--hide-on-load {
      display: none;
    }
  {% endif %}
{% endstyle %}
<div
  class="
    product
    {% if settings.enable_section_animations and section.settings.enable_animation %}
      animation
      animation--product
    {% endif %}
  "
  data-section-id="{{ section.id }}"
  data-section-type="product"
  data-current-product-id="{{ current_variant.id }}"
  data-product-has-only-default-variant="{{ product.has_only_default_variant }}"
  data-enable-sticky-container="{{ section.settings.enable_sticky_product_details }}"
  data-loop-mobile-carousel="{{ section.settings.enable_mobile_carousel_looping }}"
  data-show-mobile-carousel-full-width="{{ show_mobile_carousel_images_full_width }}"
  data-hide-mobile-carousel-dots="{{ hide_mobile_carousel_dots }}"
  data-gallery-size="{{ gallery_size }}"
  data-enable-multiple-variant-media="{{ section.settings.enable_multiple_variant_media }}"
  data-initial-media-id="{{ featured_media.id }}"
  data-is-full-product="true"
  data-product-handle="{{ product.handle }}"
>
  {% if header_block == blank %}
    {% comment %} Added in the event the product header is removed so the page has an h1 tag present {% endcomment %}
    <h1 class="visually-hidden">{{ product.title }}</h1>
  {% endif %}
  {% if section.settings.show_breadcrumbs or section.settings.show_navigation_buttons -%}
    <div class="product__breadcrumbs">
      {% render 'breadcrumbs',
        show_breadcrumbs: section.settings.show_breadcrumbs,
        show_navigation_buttons: section.settings.show_navigation_buttons
      %}
    </div>
  {%- endif %}

  <div
    class="product__top product__primary"

    data-zero-price-display="{{ settings.zero_dollar_product_price_display }}"
    data-zero-price-custom-content="{{ 'products.inventory.zero_price_custom_label' | t }}"
    data-sold-out-price-display="{{ settings.sold_out_product_price_display }}"
    data-sold-out-price-custom-content="{{ 'products.inventory.sold_out_price_custom_label' | t }}"
    data-price-display-type="{{ price_content_type }}"
  >
    <div class="product__primary-left">
        {% if gallery_style_mobile == 'carousel' -%}
          {%- comment -%} Add product header inner above carousel on mobile {%- endcomment -%}
          {%- if header_block.size > 0 -%}
            {%- for block in header_block -%}
              <div class="product__block--product-header-inner mobile-only" style="display: none;">
                {% liquid
                  capture product_title_tag_type
                    echo 'h1'
                  endcapture
                %}
                {%- if product != blank -%}
                  {%- if block.settings.show_sale_badge or block.settings.show_custom_badges -%}
                    {%-
                      render 'product-badges' with
                      prod: product,
                      show_sale_badge: block.settings.show_sale_badge,
                      show_custom_badges: block.settings.show_custom_badges,
                      show_sold_out_badge: false
                    -%}
                  {%- endif -%}
                  <div class="title-wrapper-product">
                    <{{ product_title_tag_type }} class="product__title ff-heading fs-heading-4-base fs-heading-3-base-ns">
                      {{- product.title -}}
                    </{{ product_title_tag_type }}>
                  </div>
                  {% if block.settings.show_product_rating == true %}
                    <div class="review-badge-wrapper">
                      <div class="review-counts">
                        <span class="review-stars">
                          {% render 'product-rating', prod: product %}
                        </span>
                      </div>
                    </div>
                  {% endif %}
                  <div
                    class="product__price-and-ratings"
                    data-show-product-rating="{{ block.settings.show_product_rating }}"
                  >
                    
                    <div class="product__price">
                      {%- if product != blank -%}
                        <span class="visually-hidden" data-compare-text>{{ 'products.product.regular_price' | t }}</span>
                        <s data-compare-price>
                          {%- if price_content_type == 'price' and current_variant.compare_at_price > current_variant.price -%}
                            {{- current_variant.compare_at_price | money -}}
                          {%- endif -%}
                        </s>
                        <span data-price>
                          {%- if price_content_type == 'price' %}
                            {{ current_variant.price | money -}}
                          {%- elsif price_content_type == 'custom' -%}
                            {{- custom_price_content }}
                          {% endif -%}
                        </span>

                        {%- if block.settings.show_discount -%}
                          <span data-discount-display class="product__price-displayed-discount fs-body-50">
                            {%-
                              render "get-display-discount"
                              with product: product,
                              variant: current_variant,
                              format: block.settings.discount_format
                            -%}
                          </span>
                        {%- endif -%}

                        {% render 'unit-price', item: current_variant %}
                      {%- endif -%}
                    </div>
                  </div>
                  {%- if block.settings.show_vendor or block.settings.show_sku -%}
                    <div
                      class="product__vendor_and_sku"
                      data-showing-sku="{% if block.settings.show_sku and current_variant.sku != blank %}true{% else %}false{% endif %}"
                      data-showing-vendor="{% if block.settings.show_vendor and product.vendor != blank %}true{% else %}false{% endif %}"
                    >
                      {%- if block.settings.show_vendor -%}
                        <div class="product__vendor fs-body-100 t-opacity-70">{{ product.vendor | link_to_vendor }}</div>
                      {%- endif -%}
                      {%- if block.settings.show_sku -%}
                        <div class="product__sku fs-body-25 t-opacity-60" data-product-sku>
                          {%- if current_variant.sku != blank -%}
                            {{ 'products.product.sku_with_value' | t: value: current_variant.sku }}
                          {%- endif -%}
                        </div>
                      {%- endif -%}
                    </div>
                  {%- endif -%}
                {%- endif -%}
              </div>
              <style>
                @media (max-width: 991px) {
                  .product__block--product-header-inner.mobile-only {
                    display: block !important;
                    padding: 20px;
                  }
                }
                @media (min-width: 992px) {
                  .product__block--product-header-inner.mobile-only {
                    display: none !important;
                  }
                }
              </style>
            {%- endfor -%}
          {%- endif -%}
          <div
            class="product__media-container below-mobile below-mobile-1 carousel swiper{% if section.settings.enable_product_lightbox %} lightbox-media-container{% endif %}"
            data-gallery-style="{{ gallery_style_mobile }}"
          >
            <div class="product__media carousel__wrapper swiper-wrapper">
              {%- assign media_index = 0 -%}

              {% for media in product.media -%}
                {% liquid
                  assign prioritize_loading = false

                  if forloop.first or forloop.index == initial_variant_media_position
                    assign prioritize_loading = true
                  endif

                  if forloop.index < initial_variant_media_position or forloop.index > next_variant_media_index
                    continue
                  endif

                  assign media_aspect_ratio = image_aspect_ratio

                  if media.media_type == 'model'
                    assign media_aspect_ratio = 'square'
                  endif

                  assign is_featured_media = false

                  if media == featured_media
                    assign is_featured_media = true
                  endif
                -%}
                <div
                  class="product__media-item carousel_slide swiper-slide"
                  data-is-featured="{{ is_featured_media }}"
                  data-slide-index="{{ media_index }}"
                  data-media-item-id="{{ media.id }}"
                  data-media-type="{{ media.media_type }}"
                  data-product-media-wrapper
                  data-aspect-ratio="{{ media_aspect_ratio }}"
                >
                  {% render 'media',
                    fallback_alt: product.title,
                    media: media,
                    featured_media: featured_media,
                    enable_product_lightbox: section.settings.enable_product_lightbox,
                    loop_video: section.settings.enable_video_looping,
                    aspect_ratio: media_aspect_ratio,
                    sizes: '90vw',
                    prioritize_loading: prioritize_loading,
                    within_carousel: true
                  %}
                </div>

                {%- assign media_index = media_index | plus: 1 -%}
              {%- else -%}
                <div class="product__media-item placeholder-image">
                  {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                </div>
              {%- endfor %}
            </div>

            <div class="swiper-pagination"></div>
          </div>
        {%- else -%}
          <div
            class="product__media-container below-mobile{% if section.settings.enable_product_lightbox %} lightbox-media-container{% endif %}"
            data-gallery-style="{{ gallery_style_mobile }}"
          >
            <div
              class="product__media "
              {% if over_media_show_limit_mobile %}
                data-product-media="closed"
                style="
                  --overflow-gallery-aspect-ratio: {{ overflow_gallery_aspect_ratio_mobile }};
                  --overflow-gallery-transition-multiplier: {{ transition_multiplier_mobile }};
                "
              {%- endif -%}
            >
              {% for media in product.media -%}
                {% liquid
                  if forloop.index < initial_variant_media_position or forloop.index > next_variant_media_index
                    continue
                  endif

                  assign media_aspect_ratio = image_aspect_ratio

                  if media.media_type == 'model'
                    assign media_aspect_ratio = 'square'
                  endif
                -%}
                <div
                  class="
                    product__media-item
                    {% if forloop.index0 >= media_show_limit %}product__media-item--hide-on-load{% endif %}
                    animation--product-media
                  "
                  data-media-item-id="{{ media.id }}"
                  data-media-type="{{ media.media_type }}"
                  data-product-media-wrapper
                  {% if media.media_type == 'model' -%}
                    data-aspect-ratio="{{ media_aspect_ratio }}"
                  {%- endif %}
                >
                  {% render 'media',
                    media: media,
                    fallback_alt: product.title,
                    featured_media: featured_media,
                    enable_product_lightbox: section.settings.enable_product_lightbox,
                    loop_video: section.settings.enable_video_looping,
                    aspect_ratio: media_aspect_ratio,
                    sizes: '90vw',
                    animate: true,
                    within_carousel: false
                  %}
                </div>
              {%- else -%}
                <div class="product__media-item placeholder-image">
                  {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                </div>
              {%- endfor %}

              {% if over_media_show_limit_mobile -%}
                <div class="product__media-show-more" data-more-media-bar>
                  <button
                    class="btn btn--tertiary btn--small"
                    type="button"
                    data-more-media
                    data-lang-more-media="{{ 'products.product.more_media' | t | escape }}"
                    data-lang-less-media="{{ 'products.product.less_media' | t | escape }}"
                  >
                    {{ 'products.product.more_media' | t }}
                  </button>
                </div>
              {%- endif %}
            </div>
          </div>
        {%- endif %}
  
      <div
        class="product__media-container {% if section.settings.enable_product_lightbox %} lightbox-media-container{% endif %}"
        data-gallery-style="{{ gallery_style }}"
      >
        {% if gallery_style == 'thumbnails' -%}
          {% render 'product-thumbnails-new',
            featured_media: featured_media,
            aspect_ratio: image_aspect_ratio,
            initial_variant_media_position: initial_variant_media_position,
            next_variant_media_index: next_variant_media_index
          %}
        {%- endif %}
        <div class="gallery-wrapper">
          {%- render 'product-badges',
            prod: product,
            show_sale_badge: settings.product_listing_show_sale_badge,
            show_custom_badges: settings.product_listing_show_custom_badges,
            show_sold_out_badge: settings.product_listing_show_sold_out_badge
          -%}
          {% if gallery_style == 'grid' -%}
            {%- comment -%} Grid layout: First image large, rest in grid {%- endcomment -%}
            <div class="product__media gallery-desktop gallery-desktop--grid-layout">
              {%- assign first_item_rendered = false -%}
              {% for media in product.media -%}
                {% liquid
                  if forloop.index < initial_variant_media_position or forloop.index > next_variant_media_index
                    continue
                  endif

                  assign media_aspect_ratio = image_aspect_ratio

                  if media.media_type == 'model'
                  endif
                  
                  if first_item_rendered == false
                    assign is_first_image = true
                    assign first_item_rendered = true
                  else
                    assign is_first_image = false
                  endif
                -%}
                
                {% if is_first_image -%}
                  {%- comment -%} First image - display large {%- endcomment -%}
                  <div class="product__media-first">
                    <div
                      class="
                        product__media-item product__media-item--first {% if media.image.aspect_ratio < 1 or media.image.aspect_ratio > 1.5 %} object_fit-contain {% endif %}
                        animation--product-media
                      "
                      data-media-item-id="{{ media.id }}"
                      data-media-type="{{ media.media_type }}"
                      data-product-media-wrapper
                      data-aspect-ratio="{{ media_aspect_ratio }}"
                    >
                      {% render 'media',
                        media: media,
                        fallback_alt: product.title,
                        featured_media: featured_media,
                        enable_product_lightbox: section.settings.enable_product_lightbox,
                        loop_video: section.settings.enable_video_looping,
                        aspect_ratio: media_aspect_ratio,
                        sizes: desktop_image_sizes_attribute,
                        animate: true,
                        within_carousel: false
                      %}
                    </div>
                  </div>
                  <div class="product__media-grid">
                {%- else -%}
                  {%- comment -%} Remaining images - display in grid {%- endcomment -%}
                  <div>
                    <div
                      class="
                        product__media-item product__media-item--grid {% if media.image.aspect_ratio < 1 or media.image.aspect_ratio > 1.5 %} object_fit-contain {% endif %}
                        animation--product-media
                      "
                      data-media-item-id="{{ media.id }}"
                      data-media-type="{{ media.media_type }}"
                      data-product-media-wrapper
                      data-aspect-ratio="{{ media_aspect_ratio }}"
                    >
                      {% render 'media',
                        media: media,
                        fallback_alt: product.title,
                        featured_media: featured_media,
                        enable_product_lightbox: section.settings.enable_product_lightbox,
                        loop_video: section.settings.enable_video_looping,
                        aspect_ratio: media_aspect_ratio,
                        sizes: desktop_image_sizes_attribute,
                        animate: true,
                        within_carousel: false
                      %}
                    </div>
                  </div>
                {%- endif %}
              {%- else -%}
                <div class="product__media-item placeholder-image">
                  {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                </div>
              {%- endfor %}
              {%- if first_item_rendered -%}
                </div>
              {%- endif %}
              
              {% if over_media_show_limit -%}
                <div class="product__media-show-more" data-more-media-bar>
                  <button
                    class="btn btn--tertiary btn--small"
                    type="button"
                    data-more-media
                    data-lang-more-media="{{ 'products.product.more_media' | t | escape }}"
                    data-lang-less-media="{{ 'products.product.less_media' | t | escape }}"
                  >
                    {{ 'products.product.more_media' | t }}
                  </button>
                </div>
              {%- endif %}
            </div>
          {%- else -%}
            {%- comment -%} Non-grid layout (list, thumbnails, etc.) {%- endcomment -%}
            <div
              class="product__media gallery-desktop"
              data-media-limit="{{ media_limit }}"
              {% if over_media_show_limit -%}
                data-product-media="closed"
                style="
                  --overflow-gallery-aspect-ratio: {{ overflow_gallery_aspect_ratio }};
                  --overflow-gallery-transition-multiplier: {{ transition_multiplier }};
                "
              {%- endif %}
            >
              {% for media in product.media -%}
                {% liquid
                  if forloop.index < initial_variant_media_position or forloop.index > next_variant_media_index
                    continue
                  endif

                  assign media_aspect_ratio = image_aspect_ratio

                  if media.media_type == 'model'
                  endif
                -%}
                <div>
                  <div
                    class="
                      product__media-item {% if media.image.aspect_ratio < 1 or media.image.aspect_ratio > 1.5 %} object_fit-contain {% endif %}
                      {% if forloop.index0 >= 1 %}product__media-item--hide-on-load{% endif %}
                      animation--product-media
                    "
                    data-media-item-id="{{ media.id }}"
                    data-media-type="{{ media.media_type }}"
                    data-product-media-wrapper
                    {% if gallery_style == 'grid' or media.media_type == 'image' or media.media_type == 'model' -%}
                      data-aspect-ratio="{{ media_aspect_ratio }}"
                    {%- endif %}
                  >
                    {% render 'media',
                      media: media,
                      fallback_alt: product.title,
                      featured_media: featured_media,
